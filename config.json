{
  "workspaceConfig": {
    "elementsPerPage": 20,
    "theme": "light",
    "spellcheck": false
  },
  "apiConfig": {
    "sourceFolder": "./source",
    "databaseConnectionConfig": {
      "connectionString": "sqlite:///file_category.db",
      "params": {}
    },
    "shortcuts": {
      "toggle_audio": {
        "key": "ctrl+1",
        "desc": "Toggles audio in a line where select or textarea is focused."
      },
      "move_to_textarea": {
        "key": "ctrl+2",
        "desc": "Toggles focus to textarea in focused line"
      },
      "move_to_select": {
        "key": "ctrl+3",
        "desc": "Toggles focus to select element in focused line"
      },
      "move_backwards": {},
      "stop_audio": {}
    }
  },
  "finalisationRecent": {
    "ffmpegConstructor": {
      "ac": 1,
      "ar": 22050,
      "af": "",
      "type": "wav"
    },
    "audioLengthFilter": {
      "minLength": 0,
      "maxLength": 15,
      "invalidsDir": ""
    },
    "exportType": {
      "onlyText": false,
      "optionValue": "to_one_folder"
    },
    "lineFormat": {
      "format": "wavs/{audio_name}|{text_transcript}"
    },
    "confirmChoices": {
      "filterByLength": false,
      "formatAudio": false
    }
  }
}
